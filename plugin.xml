<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="com.deepnoodle.openeditors.fragment"
         point="org.eclipse.e4.workbench.model">
      <fragment
            apply="initial"
            uri="fragment.e4xmi">
      </fragment>
   </extension>
    <extension
         point="org.eclipse.ui.views">
      <category
            name="deepnoodle"
            id="deepnoodle">
      </category>
      <view
            name="Open Editors"
            icon="icons/Sample.png"
            category="deepnoodle"
            class="com.deepnoodle.openeditors.views.openeditors.EditorsView"
            id="EditorsView">
      </view>
   </extension> 
   <extension
         point="org.eclipse.ui.ide.filesystemSupport">
      <filesystemContributor
            class="eclipsepluginv2.FileSystemContributor1"
            label="label"
            scheme="EclipsePluginV2.filesystemContributor1">
      </filesystemContributor>
   </extension>
   

</plugin>
